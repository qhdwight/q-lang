pkg main {
    dat test_d {
        u32 { a; b; }
    }
    def nil -> u32 {
        imp main {
            u32 { c <- 0; }
            test_d { t <- test_d{a <- 3; b <- 2; }; }
            c <- 3 + 2 + 3;
            t.a <- 5;
            t.b <- c + 32;
            itr 0...3 {
                c <- c + 3;
            }
            wln c;
            wln t.a;
            wln t.b;
            out 0;
        }
    }
}
