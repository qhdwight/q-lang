pkg main {
    dat test_d {
        u32 { a; b; }
    }
    def void -> u32 {
        imp main {
            u32 { c <- 0; }
            test_d { d <- test_d{a <- 3, b <- 2, }; }
            c <- 3 + 2 + 3;
            d.a <- 5;
            d.b <- c + 32;
            for 0..3 {
                c <- c + 3;
            }
            wln c;
            wln d.a;
            wln d.b;
            out 0;
        }
    }
}
